{
  "manifest_version": 3,
  "name": "Edify",
  "version": "1.0.0",
  "description": "Chrome extension to import/export YAML configurations to Dify.",
  "permissions": ["cookies", "alarms"],
  "host_permissions": [
    "*://*/*",
    "*://localhost/*",
    "*://127.0.0.1/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
